  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm start --prefix client\" \"npm run dev --prefix server\"",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write '*/**/*.{js,jsx,ts,tsx,json,css,md}'",
